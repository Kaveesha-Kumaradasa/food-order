{
  "expo": {
    "name": "food-order-app",
    "slug": "food-order-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/food-app-icon.png",
    "scheme": "foodorderapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.delivergate.demoapp",
      "icon": "./assets/images/food-app-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/food-app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/food-app-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/food-app-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "37b9d8f6-6609-4505-8a33-563b1d49b190"
      }
    }
  }
}
